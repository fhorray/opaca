{
  "name": "opaca-dev",
  "version": "0.0.9",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./codegen": {
      "import": "./dist/codegen/generate-routes.js",
      "types": "./dist/codegen/generate-routes.d.ts"
    },
    "./builder": {
      "import": "./dist/builder/cloudflare-builder.js",
      "types": "./dist/builder/cloudflare-builder.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/build-runtime.js",
      "types": "./dist/runtime/build-runtime.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json --force",
    "watch": "tsc -p tsconfig.json --watch",
    "check-types": "tsc --noEmit"
  },
  "catalog": {
    "bun": "1.2.0",
    "typescript": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/bun": "latest",
    "@cloudflare/workers-types": "^4.20251202.0",
    "bun-plugin-tailwind": "^0.1.2",
    "opaca": "workspace:*"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.1.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251202.0",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "opaca": "workspace:*",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "bun": "^1.2.0",
    "opaca": "workspace:*"
  }
